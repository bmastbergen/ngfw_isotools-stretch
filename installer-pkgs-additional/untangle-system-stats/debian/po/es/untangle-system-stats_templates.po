#. Type: note
#. Description
#. :sl1:
#: ../untangle-system-stats.templates:2001
msgid "Summary:."
msgstr "Sumario:."

# the words in {} are variables, right? If so, they should not be translated.
#. Type: note
#. Description
#. :sl1:
#: ../untangle-system-stats.templates:2001
msgid ""
" Total memory:    ${memory}MB -> ${memory_status}\n"
" Processor speed: ${cpu}MHz -> ${cpu_status}\n"
" Network cards:   ${nics} -> ${nics_status}"
msgstr ""
" Memoria total :    ${memory}MB -> ${memory_status}\n"
" Velocidad del Procesador: ${cpu}MHz -> ${cpu_status}\n"
" Tarjetas de Red:   ${nics} -> ${nics_status}"

#. Type: boolean
#. Description
#. :sl1:
#: ../untangle-system-stats.templates:3001
msgid "Do you want to proceed and format your disk?"
msgstr "Quiere proceder y formatear su disco?"

#. Type: boolean
#. Description
#. :sl1:
#: ../untangle-system-stats.templates:3001
#, fuzzy
msgid ""
" Untangle must format the disk to continue\n"
" WARNING: ALL DATA ON YOUR DISK WILL BE LOST!\n"
" ${extra_warning}"
msgstr ""
"Untangle debe formatear el disco para continuar PELIGRO: TODA LA INFORMACION "
"EN SU DISCO SERÁ DESTRUIDA! ${extra_warning}"

